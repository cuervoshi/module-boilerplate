{
  "name": "module-boilerplate",
  "version": "0.0.0",
  "description": "LaWallet Module Boilerplate",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "default": "./dist/index.js",
    "require": "./dist/index.mjs",
    "import": "./dist/index.js",
    "node": "./dist/index.mjs"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist",
    "prettier": "prettier . --write"
  },
  "keywords": [],
  "author": "Agustin Kassis",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "glob": "^10.3.4",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@nostr-dev-kit/ndk": "^0.8.23",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.83",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
