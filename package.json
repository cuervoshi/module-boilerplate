{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Wallet provider for LaWallet",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "tsup": {
    "entry": [
      "src/index.ts"
    ]
  },
  "exports": {
    "default": "./dist/index.js",
    "require": "./dist/index.mjs",
    "import": "./dist/index.js",
    "node": "./dist/index.mjs"
  },
  "scripts": {
    "test": "jest --coverage",
    "dev": "tsup src/index.ts --format esm --watch --onSuccess \"node dist/index.mjs\"",
    "build": "tsc",
    "start": "node dist/index.mjs"
  },
  "keywords": [],
  "author": "Agustin Kassis",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
